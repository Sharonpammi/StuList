<li id="<%= feed_item.id %>">
  
  <span class="content">From: <%= feed_item.user_id %></span>
  <span class="content">Subject: <%= feed_item.SubjectTitle %></span>
  <span class="content">Message: <%= feed_item.Body %></span>
  <span class="timestamp">
    Sent <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "Are you sure?" },
                                     title: feed_item.SubjectTitle %>

</li>